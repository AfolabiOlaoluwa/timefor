<% if @schedule.destroyed? %>
  $('#schedule-' +<%= @schedule.id %>).fadeOut(600);
  $.gritter.add({
    title: 'Success',
    text: 'Schedule deleted.',
    class_name: 'color success'
  });
<% else %>
  $.gritter.add({
    title: 'Warning',
    text: 'There was a problem deleting the schedule. Please try again.',
    class_name: 'color warning'
  });
<% end %>
